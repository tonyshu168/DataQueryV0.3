/******** 登录框 ***********/
.modal
{
    position : fixed;
    left : 0;
    right: 0;
    top : 0;
    bottom : 0;
    background : rgba(153,197,157,  1);
    /*opacity :  会导致所有内容全部半透明*/
    /*display : none;*/
}

.modal-dialog
{
    position: absolute;
    left:50%;
    top: 38.2%;
    width : 350px;
    /*margin : 100px auto;*/
    margin-left:-175px;
    margin-top:-147px;
}

.modal-content
{
    background : #fff;
    min-height : 100px;
    border-radius : 3px;
}

.modal-content span
{
    display : block;
    margin-left : 20px;
    margin-bottom : 20px;
    padding-top : 10px;
    font-size : 16px;
}

#login-form input
{
    border : 1px solid #aaa;
    margin-left : 20px;
    margin-bottom : 20px;
    width : 305px;
    padding : 8px 5px;
}

input.alert
{
    background-color : #FEEAE1;
    color : #BC7C76;
    border-color: #C69594;
}
#user
{
    border-color : #98A7BE;
}
#submit
{
    text-align : center;
    background : #f00;
    color : #fff;
}
/***用户登录提醒 ***/
.msg-warn{
    width:305px;
    margin-left:20px;
    margin-bottom:10px;
    padding:5px 0;
    padding-left:5px;
    border:1px solid #eee;
    background:#FFF6D2;
}

.msg-warn b{
    position:relative;
    top:2px;
    display:inline-block;
    width:20px;
    height:17px;
    background:url("../img/pwd-icons-new01.png") no-repeat -24px -299px;
}

/***密码输入错误时提醒框 ***/
.msg-error{
    width:305px;
    margin-left:20px;
    margin-bottom:10px;
    padding:5px 0;
    padding-left:5px;
    border:1px solid #faccc6;
    background:#FFEBEB;
    color:#e4393c;
}

.msg-error b{
    position:relative;
    top:2px;
    display:inline-block;
    width:22px;
    height:16px;
    background:url("../img/pwd-icons-new01.png") no-repeat -0px -350px;
}

/****有户登录的左边小图****/
.icon-uname{
    background:url("../img/pwd-icons-new01.png") no-repeat 0 0;
}
.icon-pwd{
    background:url("../img/pwd-icons-new01.png") no-repeat 0 -52px;
}
#login-form input.icon-uname{padding-left:44px;}

#login-form input.icon-pwd{padding-left:44px;}

#login-form div{
    padding:0;
    margin:0;
    margin-left:20px;
    margin-bottom:20px;
}
#login-form div input{
    margin:0;
    padding:0;
    width:20px;
    vertical-align:middle;
}

/******** register user ***************/
.input{
    border:1px solid #aaa;
    margin-left : 20px;
    margin-bottom : 5px;
    margin-top:10px;
    width : 310px;
    padding-left:5px;
    padding-right:0;
    overflow:hidden;
}
.regInput{
    width:180px;
    padding:8px 5px;
    outline:none;
}

.prompt{
    display:inline-block;
    margin-left:20px;
    width:13px;
    height:13px;
    background:url("../img/pwd-icons-new01.png") no-repeat -26px -400px;
}

.submit
{
    text-align : center;
    background : #f00;
    color : #fff;
    font-size:1.3em;
    font-weight:bold;
    padding:8px;
    margin-bottom:20px;
}
.registerCheckbox {
    vertical-align:middle;
    position:relative;
    top:-2px;
    margin-left:20px;
    width:18px;
    height:18px;
}
canvas:hover{
    cursor:pointer;
}

.button{
    border:none;
    font-weight:bold;
    margin:0;
    padding:7px 11px;
}

div.input button:hover{cursor:pointer;}
/***********register end****************/


.navbar-default .navbar-text{color:#3879D9; font-weight:bold;}

.navbar-default .navbar-nav >li >a{color:#3879D9;}

.navbar-default .navbar-nav >li>a:hover{ color:#f00;cursor:pointer;}

/*** 修改密码框 ********/
#changePwd{
    display:none;
    top:220px;
    /*top:50px;
    left: 38%;*/
    left:50%;
    margin-left: -175px;
}

.title{
    margin-right:20px;
}
.title:after{
    content:"";
    display:block;
    clear:both;
}

.close{
    width:31px;
    height:31px;
    line-height:15px;
    text-align:center;
}
.return{
    text-align : center;
    font-size:1.3em;
    font-weight:bold;
    margin-top:-8px;
    padding:8px;
    margin-bottom:20px;
}

#confirm{
    margin-right:20px;
    margin-bottom:20px;
}

#confirm:after{
    content:"";
    display:block;
    clear:both;
}

.inputbtn{
    border:1px solid #aaa;
    margin-left : 20px;
    margin-bottom : 5px;
    margin-top:10px;
    width:43%;
    padding-left:5px;
    padding-right:0;
}

.regSelect{
    width:180px;
    padding:8px 5px;
    border:none;
    outline:none;
}
/****** 输入框的成功与否的 ****/
.marginLeft{
    font-size : 18px;
    margin-left:20px;
}

/** 编辑窗口 **/
#editWindow,.editWindow{
    display:none;
    top:220px;
    left: 50%;
}
._modal-dialog{
    position :absolute;
}

#editConfirm{
    margin-right:20px;
    margin-bottom:20px;
}

#editConfirm:after{
    content:"";
    display:block;
    clear:both;
}

/***弹出框(查找不到数据时) **/
#selectAlert {
    display:none;
    position:absolute;
    top:390px;
    left:40%;
}
.alertText{
    font-size:1.3em;
    color: #f00;
    margin-bottom:30px;
    text-align:center;
}

#table-data tbody tr td:first-child input {width:87px;}

#table-data tbody tr td input{
    background:transparent;
    text-align:center;
    width:80px;
    outline:none;
}

.a_link{
    display:block;
    width:40px;
}

/* 重新设置用户密码*/
#resetPwd{
   display:none;
    top:220px;
    left: 50%;
    margin-left:-175px;
}

#confirm_reset{
    margin-right:20px;
    margin-bottom:20px;
}

#confirm_reset:after{
    content:"";
    display:block;
    clear:both;
}


/**** 合作列表的弹出框 *********/
.selectAlert {
    display:none;
    position:absolute;
    top:380px;
    left:50%;
}

/********* 新增输入框自动补全 ********/
.autoAppend{
    display:none;
    float:left;position:absolute;top:25px;left:60px; background:#fff;
    font-size:15px;
    z-index:1;
}

.autoAppend li{
    width:170px;height:36px;padding-left:5px;line-height:36px;
}

.autoAppend li:hover,.hover{
    background:#eee;
    color:#5FA7FD;
}
#eidtBack-form div.input{overflow:visible;}

/*分页显示 */
.pager_custom
{
    list-style : none;
    padding : 0;
    overflow: hidden;
    margin-top : 10px;
    margin-bottom:20px;
}

.pager_custom li
{
    display : inline-block;
    border : 1px solid #ddd;
    /*padding : 8px 10px;*/
    border-radius: 3px;
    margin : 0 5px;
    overflow:hidden;
}
.pager_custom li > a{
    display : inline-block;
    padding:8px 10px;
}

.pager_custom li.active
{
    border : none;
}

.pager_custom li.active a
{
    color : #e4393c;
    font-weight : bold;
    cursor : default;
    text-decoration: none;
}






